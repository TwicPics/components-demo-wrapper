{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@twicpics/components": "^0.6.0",
    "@twicpics/components-demo-wrapper": "git+https://github.com/TwicPics/components-demo-wrapper.git#main", 
    "react": "17.0.2",
    "react-app-rewired": "2.1.8",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "build": "$npm_execpath install && DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true react-app-rewired build",
